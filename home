<script>
document.addEventListener('DOMContentLoaded', (event) => {
    const logo = document.querySelector('.logo');
    logo.style.opacity = '0';
    logo.style.transform = 'translateZ(-1000px) scale(0.1)';
    logo.style.color = '#333333';

    setTimeout(() => {
        logo.style.transition = 'opacity 3s, transform 3s, color 3s';
        logo.style.opacity = '1';
        logo.style.transform = 'translateZ(0) scale(1)';
        logo.style.color = '#cccccc';
    }, 100);
});
</script>

